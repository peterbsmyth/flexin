{
  "compileOnSave": false,
  "compilerOptions": {
    "resolveJsonModule": true,
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@bod/shared/environments": ["libs/shared/environments/src/index.ts"],
      "@bod/training/domain": ["libs/training/domain/src/index.ts"],
      "@bod/shared/domain": ["libs/shared/domain/src/index.ts"],
      "@bod/shared/models": ["libs/shared/models/src/index.ts"],
      "@bod/shared/components": ["libs/shared/components/src/index.ts"],
      "@bod/celebrities/domain": ["libs/celebrities/domain/src/index.ts"],
      "@bod/celebrities/feature-display-top-ten": [
        "libs/celebrities/feature-display-top-ten/src/index.ts"
      ],
      "@bod/shared/scripts": ["libs/shared/scripts/src/index.ts"],
      "@bod/shared/utils": ["libs/shared/utils/src/index.ts"],
      "@bod/training/feature-auth": ["libs/training/feature-auth/src/index.ts"],
      "@bod/training/feature-manage-programs": [
        "libs/training/feature-manage-programs/src/index.ts"
      ],
      "@bod/training/feature-manage-exercises": [
        "libs/training/feature-manage-exercises/src/index.ts"
      ],
      "@bod/training/feature-manage-workouts": [
        "libs/training/feature-manage-workouts/src/index.ts"
      ],
      "@bod/training/ui-components": [
        "libs/training/ui-components/src/index.ts"
      ],
      "@bod/training/feature-manage-categories": [
        "libs/training/feature-manage-categories/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
